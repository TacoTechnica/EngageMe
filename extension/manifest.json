{
  "name": "EngageMe",
  "version": "0.0.1.1",
  "description": "Analyzes YouTube Engagement, save time!",
  "permissions": ["storage", "tabs"],
  "background": {
    "matches": ["*://www.youtube.com/*", "*://leccap.engin.umich.edu/leccap/*"],
    "scripts": ["jquery-3.4.1.min.js", "background_tracker.js"]
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*",
        "*://leccap.engin.umich.edu/leccap/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "user.js",
        "display.js",
        "tracker.js",
        "main.js"
      ]
    }
  ],
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
