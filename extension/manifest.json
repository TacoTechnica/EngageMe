{
  "name": "EngageMe Test",
  "version": "1.10.37",
  "description": "Analyzes YouTube Engagement, save time!",
  "permissions": ["storage", "tabs", "declarativeContent"],
  "background": {
    "matches": ["*://www.youtube.com/*", "*://leccap.engin.umich.edu/leccap/*"],
    "scripts": [
      "jquery-3.4.1.min.js",
      "globals.js",
      "background/popup_config.js",
      "background/background.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "*://www.youtube.com/*",
        "*://leccap.engin.umich.edu/leccap/*"
      ],
      "js": [
        "jquery-3.4.1.min.js",
        "globals.js",
        "content/content_globals.js",
        "content/tracker.js",
        "content/display.js",
        "content/content_globals_init.js"
      ]
    }
  ],
  "browser_action": {
    "default_popup": "popup/popup.html"
  },
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
