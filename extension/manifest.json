{
  "name": "EngageMe",
  "version": "1.10.37",
  "description": "Analyzes YouTube Engagement, save time!",
  "permissions": [
    "storage",
    "tabs"
  ],
  "background": {
    "matches": [
      "https://www.youtube.com/watch*",
      "https://leccap.engin.umich.edu/leccap/*"
    ],
    "scripts":[
      "lib/jquery.min.js",
      "background_tracker.js",
      "background_display.js"
    ]
  },
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/watch*",
        "https://leccap.engin.umich.edu/leccap/*"
      ],
      "js": [
        "lib/jquery.min.js",
        "user.js",
        "display.js",
        "tracker.js",
        "main.js"
      ]
    }
  ],
  "icons": {
    "64": "icon64.png",
    "128": "icon128.png"
  },
  "manifest_version": 2
}
